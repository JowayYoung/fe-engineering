{
	"name": "node-esm",
	"version": "1.0.0",
	"main": "src/index.js",
	"scripts": {
		"start": "nodemon -x babel-node src/index.js"
	},
	"dependencies": {
		"trample": "0.4.0"
	},
	"devDependencies": {
		"@babel/cli": "7.16.0",
		"@babel/core": "7.16.5",
		"@babel/node": "7.16.5",
		"@babel/preset-env": "7.16.5",
		"nodemon": "2.0.15"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		]
	},
	"nodemonConfig": {
		"env": {
			"NODE_ENV": "dev"
		},
		"execMap": {
			"js": "node --harmony"
		},
		"ext": "js json",
		"ignore": [
			"dist/"
		],
		"watch": [
			"src/"
		]
	}
}